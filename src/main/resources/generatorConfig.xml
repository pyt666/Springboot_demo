<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
  <context id="context1">
    <jdbcConnection connectionURL="jdbc:mysql://127.0.0.1:3306/spring_boot_test"
    	 driverClass="com.mysql.jdbc.Driver" 
    	 password="1234" 
    	 userId="root" />
   	<!-- 生成模型的包名和位置 -->
    <javaModelGenerator targetPackage="com.example.Springbootdemo.pojo" targetProject="Springboot-demo\src\main\java" />
    <!-- 生成的映射文件包名和位置 -->
    <sqlMapGenerator targetPackage="mappers" targetProject="Springboot-demo\src\main\resources" />
    <!-- 生成DAO的包名和位置 -->
    <javaClientGenerator targetPackage="com.example.Springbootdemo.dao" targetProject="Springboot-demo\src\main\java" type="XMLMAPPER" />
    <table tableName="appuser">
    </table>
  </context>
</generatorConfiguration>